
@{
    ViewData["Title"] = "Map";
}

<h1>Standorte</h1>

<div id="googleMap" style="width:100%;height:800px;"></div>

<script>
function myMap() {
    var mapProp= {
        center: new google.maps.LatLng(47.264816, 11.389366),
        zoom:15,
    };
    var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
    var marker = new google.maps.Marker({
        position: new google.maps.LatLng(47.264816, 11.389366),
        map: map,
    });
    marker.setMap(map);
    
}
    
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD1E1BgXWjy23YdtrEYgX56GE_wC9Mg9pw&callback=myMap"></script>